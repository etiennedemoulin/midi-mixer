{
  "name": "midi-mixer",
  "description": "soundworks application",
  "authors": [],
  "license": "BSD-3-Clause",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "npm run clean && sass src/clients/components/css:.build/public/css && soundworks-build -b",
    "clean": "soundworks-build -D",
    "dev": "npm run build && (concurrently -i -p \"none\" \"npm run watch:inspect server\" \"soundworks-build -b -w\" \"npm run watch:sass\")",
    "postinstall": "soundworks-build -C",
    "start": "node .build/server/index.js",
    "watch": "soundworks-build -p",
    "watch:inspect": "soundworks-build -d -p",
    "watch-build": "soundworks-build -b -w",
    "watch:sass": "sass --watch src/clients/components/css:.build/public/css",
    "lint": "eslint .",
    "electron:dev": "soundworks-electron dev",
    "electron:build": "soundworks-electron build",
    "max:dev": "esbuild ./src/server/index.js  --bundle --watch --format=cjs --platform=node --external:max-api --outfile=max/midi-mixer/javascript/midi-mixer.cjs",
    "max:build": "esbuild ./src/server/index.js  --bundle --format=cjs --platform=node --external:max-api --outfile=max/midi-mixer/javascript/midi-mixer.cjs"
  },
  "dependencies": {
    "@ircam/sc-components": "^3.0.0-alpha.50",
    "@soundworks/core": "^4.0.0-alpha.17",
    "@soundworks/helpers": "^1.0.0-alpha.10",
    "@soundworks/plugin-filesystem": "^2.0.0-alpha.6",
    "electron-rebuild": "^3.2.9",
    "esbuild": "^0.19.9",
    "json5": "^2.2.3",
    "jzz": "^1.6.4",
    "lit": "^3.1.0",
    "lodash": "^4.17.21",
    "node-osc": "^9.0.2"
  },
  "devDependencies": {
    "@ircam/eslint-config": "^1.3.0",
    "@soundworks/build": "^1.0.0-alpha.0",
    "@soundworks/create": "^1.0.0-alpha.14",
    "@soundworks/electron": "^0.3.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.41.0",
    "sass": "^1.57.1"
  }
}
